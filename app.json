{
  "expo": {
    "name": "Cobby",
    "slug": "cobby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cobbyai.app",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "The app needs access to your photos to scan cards.",
        "NSCameraUsageDescription": "The app needs access to your camera to scan cards."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cobbyai.app",
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to scan cards.",
          "cameraPermission": "The app needs access to your camera to scan cards."
        }
      ]
    ],
    "scheme": "cobbyaiapp",
    "experiments": {
      "tsconfigPaths": true
    },
    "newArchEnabled": true,
    "assets": [
      "./assets/images/cards-bg.jpg"
    ]
  }
} 